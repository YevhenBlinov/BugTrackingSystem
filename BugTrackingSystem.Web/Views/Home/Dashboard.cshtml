@model IEnumerable<BugTrackingSystem.Service.Models.BaseBugViewModel>
<body>
    <div class="general-container marginFromMenu">
        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-3 padding0">
                <h1 id="MyProjects" style="">My projects</h1>
                @Html.Action("MyProjects", "Home")

                <div class="container col-lg-12">
                    <a href="@Url.Action("Index", "Project")" id="ShowAllMyProjects">Show all<i class="fa fa-long-arrow-right" aria-hidden="true"></i></a>
                </div>
            </div>

            <div id="UserBugsInDashBoard" class="col-lg-9 col-md-9 col-sm-9">
                <h1 id="AssignedToMe">Assigned to me</h1>
                @Html.Action("UserBugs", "Shared")
            </div>
        </div>
    </div>
</body>
